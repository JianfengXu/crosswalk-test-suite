<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite name="webapi-usecase-tests">
    <set name="Multimedia &amp;amp; Graphics">
      <testcase component="usecase" execution_type="manual" id="Jsenhance" purpose="Jsenhance">
      </testcase>
    </set>
    <set name="Networking &amp;amp; Storage">
      <testcase component="usecase" execution_type="manual" id="FileSystem" purpose="File System">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="RawSockets" purpose="Raw Sockets">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="WebSocket" purpose="WebSocket">
      </testcase>
    </set>
    <set name="Security">
      <testcase component="usecase" execution_type="manual" id="CSP" purpose="CSP" subcase="9">
      </testcase>
    </set>
    <set name="Tizen Tizen">
      <testcase component="usecase" execution_type="manual" id="Tizen" purpose="Tizen">
      </testcase>
    </set>
    <set name="Tizen Application">
      <testcase component="usecase" execution_type="manual" id="ApplicationManager" purpose="ApplicationManager">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="DataControl" purpose="DataControl">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Package" purpose="Package">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="ApplicationSwitch" purpose="ApplicationSwitch">
      </testcase>
    </set>
    <set name="Tizen Communication">
      <testcase component="usecase" execution_type="manual" id="NBS" purpose="NBS">
        <capability name="nbs"/>
      </testcase>
    </set>
    <set name="Tizen Content">
      <testcase component="usecase" execution_type="manual" id="Content" purpose="Content">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Download" purpose="Download">
      </testcase>
    </set>
    <set name="Tizen I/O">
      <testcase component="usecase" execution_type="manual" id="MessagePort" purpose="MessagePort">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="TizenFileSystem" purpose="Tizen Filesystem">
      </testcase>
    </set>
    <set name="Tizen Social">
      <testcase component="usecase" execution_type="manual" id="Bookmark" purpose="Bookmark">
      </testcase>
    </set>
    <set name="Tizen System">
      <testcase component="usecase" execution_type="manual" id="SystemInfo" purpose="SystemInfo">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="SystemSettingIncomingCall" purpose="SystemSetting IncomingCall">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="SystemSettingScreen" purpose="SystemSetting Screen">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Time" purpose="Time">
      </testcase>
    </set>
    <set name="Tizen User Interface">
      <testcase component="usecase" execution_type="manual" id="Notification" purpose="Notification">
      </testcase>
    </set>
    <set name="Tizen Others">
      <testcase component="usecase" execution_type="manual" id="AudioSystem" purpose="AudioSystem">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="SSO" purpose="SSO">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Vehicle" purpose="Vehicle">
      </testcase>
    </set>
  </suite>
</test_definition>
